{
  "name": "resume",
  "version": "1.0.0",
  "description": "Alan Ning's resume",
  "main": "index.js",
  "scripts": {
    "build:index" : "hackmyresume build ./resume.json TO index.html -t modern",
    "build:paper" : "hackmyresume build ./resume.json TO gen/resume.all -t node_modules/jsonresume-theme-paper",
    "build": "npm run build:index && npm run build:paper",
    "analyze": "hackmyresume analyze ./resume.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/askldjd/resume.git"
  },
  "author": "Alan Ning",
  "bugs": {
    "url": "https://github.com/askldjd/resume/issues"
  },
  "homepage": "https://github.com/askldjd/resume#readme",
  "dependencies": {
    "hackmyresume": "^1.9.0-beta",
    "jsonresume-theme-elegant": "^1.11.2",
    "jsonresume-theme-kwan": "0.0.2",
    "jsonresume-theme-onepage": "0.0.3",
    "jsonresume-theme-onepage-ds": "0.0.1",
    "jsonresume-theme-paper": "^0.3.5",
    "jsonresume-theme-slick": "^0.1.16"
  }
}
